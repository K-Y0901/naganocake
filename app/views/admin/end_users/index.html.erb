<div class="ad-index">
<h4>会員一覧</h4>
<table >
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>


<tbody>
  <% @end_users.each do |end_user| %>
      <tr>
        <td><%= end_user.id %></td>
        <td><%= end_user.last_name %><%= end_user.first_name %></td>
        <td><%= end_user.email %></td>
        <td><%= end_user.is_active %></td>
      </tr>
  <% end %>
</tbody>

</table>

</div>